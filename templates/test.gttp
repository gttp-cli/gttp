$Name: text // The name of the user
$Age: number // The age of the user
$Option: select // Option chosen by the user
  - Option 1
  - Option 2
  - Option 3
$Multi: multiselect // Multiple options chosen by the user
  - Option 1
  - Option 2
  - Option 3
$Bool: boolean // Some boolean value

---

Hello, ${Name}!
You are ${Age} years old.
Select result ${Option}.
Multiselect result: ${Multi}
Multiselect joined result: ${join(Multi, ", ")}

{{range $val := .Multi}}
    Selected: {{$val}}
{{end}}

Boolean: ${Bool ? 'yes' : 'no'}
